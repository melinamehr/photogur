<h1>New User</h1>


<%= form_for(@user, url: users_path) do |f| %>

  <div>
    <%= f.label :email %>
    <%= f.email_field :email %>
  </div>
  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <div>
    <%= f.label :password_confirmation %>
    <%= f.password_field :password %>
  </div>
  <%= f.submit "Submit !" %>


  <% end %>
